{
  "name": "nuxt-spritesmith",
  "version": "1.0.0",
  "description": "A Nuxt(3) module for generate sprite image icons via spritesmith.",
  "author": {
    "name": "elenh",
    "email": "yisiwings@163.com"
  },
  "license": "MIT",
  "type": "module",
  "main": "./dist/module.mjs",
  "types": "./dist/types.d.mts",
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "default": "./dist/module.mjs"
    },
    "./*": [
      "./*",
      "./*.d.ts"
    ]
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "nuxt3",
    "nuxt-module",
    "spritesmith",
    "sprite-icon",
    "vite",
    "nuxt-module"
  ],
  "homepage": "https://github.com/yisibell/nuxt-spritesmith",
  "bugs": {
    "url": "https://github.com/yisibell/nuxt-spritesmith/issues"
  },
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "release:lint:test": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@nuxt/kit": "^3.16.0",
    "chokidar": "^4.0.3",
    "fast-glob": "^3.3.3",
    "fs-extra": "^11.3.0",
    "spritesmith": "^3.5.1"
  },
  "devDependencies": {
    "@nuxt/devtools": "2.0.0-beta.3",
    "@nuxt/eslint-config": "^0.7.5",
    "@nuxt/module-builder": "0.8.4",
    "@nuxt/schema": "^3.15.2",
    "@nuxt/test-utils": "^3.15.4",
    "@types/fs-extra": "^11.0.4",
    "@types/spritesmith": "^3.4.5",
    "changelogen": "~0.5.7",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.18.0",
    "eslint-plugin-format": "^1.0.1",
    "nuxt": "^3.16.0",
    "prettier": "^3.4.2",
    "typescript": "~5.6.2",
    "vitest": "^3.0.3",
    "vue-tsc": "^2.2.0"
  }
}